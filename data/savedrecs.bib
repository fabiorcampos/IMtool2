@inproceedings{ ISI:000396395700028,
Author = {Troesterer, Sandra and Gaertner, Magdalena and Mirnig, Alexander and
   Meschtscherjakov, Alexander and McCall, Rod and Louveton, Nicolas and
   Tscheligi, Manfred and Engel, Thomas},
Book-Author = {{Green, P
   Boll, S
   Burnett, G
   Gabbard, J
