@article{ ISI:000396141300021,
Author = {Jo, Kichun and Lee, Minchul and Kim, Junsoo and Sunwoo, Myoungho},
Title = {{Tracking and Behavior Reasoning of Moving Vehicles Based on Roadway
   Geometry Constraints}},
Journal = {{IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS}},
