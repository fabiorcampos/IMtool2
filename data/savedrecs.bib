@inproceedings{ ISI:000390845600140,
Author = {Li, Liang and Yang, Ming and Wang, Chunxiang and Wang, Bing},
Book-Group-Author = {{IEEE}},
Title = {{Road DNA Based Localization for Autonomous Vehicles}},
