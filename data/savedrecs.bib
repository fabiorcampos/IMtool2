@article{ ISI:000382502500005,
Author = {Choe, Ronald and Puig-Navarro, Javier and Cichella, Venanzio and Xargay,
   Enric and Hovakimyan, Naira},
Title = {{Cooperative Trajectory Generation Using Pythagorean Hodograph Bezier
   Curves}},
