@inproceedings{ ISI:000390845600137,
Author = {Ward, Erik and Folkesson, John},
Book-Group-Author = {{IEEE}},
Title = {{Vehicle localization with low cost radar sensors}},
Booktitle = {{2016 IEEE INTELLIGENT VEHICLES SYMPOSIUM (IV)}},
Series = {{IEEE Intelligent Vehicles Symposium}},
Year = {{2016}},
Pages = {{864-870}},
Note = {{IEEE Intelligent Vehicles Symposium (IV), Gothenburg, SWEDEN, JUN 19-22,
   2016}},
Organization = {{IEEE}},
Abstract = {{Autonomous vehicles rely on GPS aided by motion sensors to localize
   globally within the road network. However, not all driving surfaces have
   satellite visibility. Therefore, it is important to augment these
   systems with localization based on environmental sensing such as
   cameras, lidar and radar in order to increase reliability and
   robustness. In this work we look at using radar for localization. Radar
   sensors are available in compact format devices well suited to
   automotive applications. Past work on localization using radar in
   automotive applications has been based on careful sensor modeling and
   Sequential Monte Carlo, (Particle) filtering. In this work we
   investigate the use of the Iterative Closest Point, ICP, algorithm
   together with an Extended Kalman filter, EKF, for localizing a vehicle
   equipped with automotive grade radars. Experiments using data acquired
   on public roads shows that this computationally simpler approach yields
   sufficiently accurate results on par with more complex methods.}},
