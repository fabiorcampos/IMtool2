@article{ ISI:000400685400009,
Author = {Htike, Kyaw Kyaw},
Title = {{Hidden-layer Ensemble Fusion of MLP Neural Networks for Pedestrian
   Detection}},
Journal = {{INFORMATICA-JOURNAL OF COMPUTING AND INFORMATICS}},
