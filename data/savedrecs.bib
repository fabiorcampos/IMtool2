@article{ ISI:000371982600022,
Author = {Lam, Albert Y. S.},
Title = {{Combinatorial Auction-Based Pricing for Multi-Tenant Autonomous Vehicle
   Public Transportation System}},
Journal = {{IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS}},
Year = {{2016}},
Volume = {{17}},
