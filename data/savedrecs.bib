@article{ ISI:000404299300002,
Author = {Guler, Samet and Fidan, Baris and Dasgupta, Soura and Anderson, Brian D.
   O. and Shames, Iman},
Title = {{Adaptive Source Localization Based Station Keeping of Autonomous
   Vehicles}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
