@inproceedings{ ISI:000400048106073,
Author = {Somanath, Amith and Karaman, Sertac},
Book-Group-Author = {{IEEE}},
Title = {{An Optimal Randomized Policy for Controlling Stochastic Growth Processes
   on Lattices}},
Booktitle = {{2016 IEEE 55TH CONFERENCE ON DECISION AND CONTROL (CDC)}},
