@inproceedings{ ISI:000390845600082,
Author = {Abualhoul, Mohammad Y. and Shagdar, Oyunchimeg and Nashashibi, Fawzi},
Book-Group-Author = {{IEEE}},
Title = {{Visible Light Inter-Vehicle Communication for Platooning of Autonomous
   Vehicles}},
