   Ju, Wendy},
Book-Group-Author = {{IEEE}},
Title = {{Ghost Driver: A Field Study Investigating the Interaction Between
   Pedestrians and Driverless Vehicles}},
Booktitle = {{2016 25TH IEEE INTERNATIONAL SYMPOSIUM ON ROBOT AND HUMAN INTERACTIVE
   COMMUNICATION (RO-MAN)}},
Series = {{IEEE RO-MAN}},
Year = {{2016}},
