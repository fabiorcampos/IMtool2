Title = {{Adaptive Parameter based Particle Swarm Optimisation for Accelerometer
   Calibration}},
Booktitle = {{PROCEEDINGS OF THE FIRST IEEE INTERNATIONAL CONFERENCE ON POWER
   ELECTRONICS, INTELLIGENT CONTROL AND ENERGY SYSTEMS (ICPEICES 2016)}},
Year = {{2016}},
