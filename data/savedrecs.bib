@inproceedings{ ISI:000404958000061,
Author = {Iglinski, Hubert and Babiak, Maciej},
Editor = {{Bujnak, J and Guagliano, M}},
Title = {{Analysis of the potential of autonomous vehicles in reducing the
   emissions of greenhouse gases in road transport}},
Booktitle = {{12TH INTERNATIONAL SCIENTIFIC CONFERENCE OF YOUNG SCIENTISTS ON
