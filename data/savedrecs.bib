@inproceedings{ ISI:000404343800174,
Author = {Ali, Alaa and Olaleye, Oladiran G. and Bayoumi, Magdy},
Book-Group-Author = {{IEEE}},
Title = {{Fast Region-Based DPM Object Detection for Autonomous Vehicles}},
Booktitle = {{2016 IEEE 59TH INTERNATIONAL MIDWEST SYMPOSIUM ON CIRCUITS AND SYSTEMS
   (MWSCAS)}},
Series = {{Midwest Symposium on Circuits and Systems Conference Proceedings}},
Year = {{2016}},
Pages = {{691-694}},
Note = {{59th IEEE International Midwest Symposium on Circuits and Systems
   (MWSCAS), Abu Dhabi, U ARAB EMIRATES, OCT 16-19, 2016}},
Organization = {{IEEE}},
Abstract = {{Real-time object detection is essential for self-driving cars and
   traffic monitoring. Deformable part model (DPM) is a popular competitive
   detector for its burden of intensive computations. We present a fast
   category independent object detector based on integrated modules to
   speedup DPM steps. An Edge Boxes region proposal technique is integrated
   with a feature pyramid approximation and neighborhood aware cascade to
   accelerate DPM with a comparable precision. Moreover, we employ assembly
   and SSE optimization with multi-threading to perform detection in
   real-time. Our experiments on different benchmark datasets show that our
   proposed framework is 59 times faster than original DPM using a single
   core. Meanwhile, our optimized implementation can process Pascal VOC at
   47Hz and a VGA image at 40HZ with about 2\% loss in accuracy.}},
