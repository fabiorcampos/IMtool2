@inproceedings{ ISI:000388376101160,
Author = {Gallehdari, Zahra and Meskin, Nader and Khorasani, Khashayar},
Book-Group-Author = {{IEEE}},
Title = {{Control Reconfiguration for Multiple Autonomous Vehicles Subject to
   Actuator Faults and Disturbances}},
Booktitle = {{2016 AMERICAN CONTROL CONFERENCE (ACC)}},
