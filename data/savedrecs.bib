@inproceedings{ ISI:000392215500003,
Author = {Xiong, Zhitao and Dixit, Vinayak V. and Waller, S. Travis},
Book-Group-Author = {{IEEE}},
Title = {{The Development of an Ontology for Driving Context Modelling and
   Reasoning}},
