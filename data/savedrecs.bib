@inproceedings{ ISI:000391921704112,
Author = {Shin, Seho and Ahn, Joonwoo and Park, Jaeheung},
Book-Group-Author = {{IEEE}},
Title = {{Desired Orientation RRT (DO-RRT) for Autonomous Vehicle in Narrow
   Cluttered Spaces}},
