@inproceedings{ ISI:000392215500230,
Author = {Tuncali, Cumhur Erkan and Pavlic, Theodore P. and Fainekos, Georgios},
Book-Group-Author = {{IEEE}},
Title = {{Utilizing S-TaLiRo as an Automatic Test Generation Framework for
   Autonomous Vehicles}},
