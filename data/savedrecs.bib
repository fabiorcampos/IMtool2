@inproceedings{ ISI:000400048102118,
Author = {Gregoire, Jean and Frazzoli, Emilio},
Book-Group-Author = {{IEEE}},
Title = {{Hybrid Centralized/Distributed Autonomous Intersection Control: using a
   Job Scheduler as a Planner and Inheriting its Efficiency Guarantees}},
Booktitle = {{2016 IEEE 55TH CONFERENCE ON DECISION AND CONTROL (CDC)}},
Series = {{IEEE Conference on Decision and Control}},
Year = {{2016}},
