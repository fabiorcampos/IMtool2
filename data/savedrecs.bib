@inproceedings{ ISI:000392215500199,
Author = {Klingelschmitt, Stefan and Willert, Volker and Eggert, Julian},
Book-Group-Author = {{IEEE}},
Title = {{Probabilistic, Discriminative Maneuver Estimation in Generic Traffic
   Scenes using Pairwise Probability Coupling}},
