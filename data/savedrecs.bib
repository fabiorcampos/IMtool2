@inproceedings{ ISI:000390845600045,
Author = {Kusenbach, Michael and Himmelsbach, Michael and Wuensche, Hans-Joachim},
Book-Group-Author = {{IEEE}},
Title = {{A New Geometric 3D LiDAR Feature for Model Creation and Classification
   of Moving Objects}},
Booktitle = {{2016 IEEE INTELLIGENT VEHICLES SYMPOSIUM (IV)}},
Series = {{IEEE Intelligent Vehicles Symposium}},
Year = {{2016}},
Pages = {{272-278}},
Note = {{IEEE Intelligent Vehicles Symposium (IV), Gothenburg, SWEDEN, JUN 19-22,
   2016}},
Organization = {{IEEE}},
Abstract = {{In this paper, we introduce a new geometric 3D feature combined with a
   clustering approach. Besides 3D data provided by a LiDAR point cloud,
   reflectivity information is used to further enhance the descriptivity of
   the feature. The proposed feature can be extracted and compared in
   real-time. Similar parts of an object, such as features belonging to an
   automobile headlight, are automatically clustered in an object model
   without explicit specification. Additionally, we provide a method for
   autonomous vehicles to automatically learn the shapes of observed moving
   objects and use them for real-time classification. The resulting object
   models consisting of the extracted feature clusters are interpretable by
