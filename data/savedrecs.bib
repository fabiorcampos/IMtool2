Title = {{A Survey on the Coordination of Connected and Automated Vehicles at
   Intersections and Merging at Highway On-Ramps}},
Journal = {{IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS}},
Year = {{2017}},
Volume = {{18}},
