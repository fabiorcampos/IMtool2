@article{ ISI:000402050300023,
Author = {Naranjo-Hernandez, Jose E. and Jimenez-Alonso, Felipe and
   Clavijo-Jimenez, Miguel and Gomez-Casado, Oscar},
Title = {{Reference element guidance of autonomous vehicles using 3D laser scanner}},
