@article{ ISI:000356295800013,
Author = {Schaeffer-Filho, Alberto and Lupu, Emil and Sloman, Morris},
Title = {{Federating Policy-Driven Autonomous Systems: Interaction Specification
   and Management Patterns}},
Journal = {{JOURNAL OF NETWORK AND SYSTEMS MANAGEMENT}},
