@article{ ISI:000384588600004,
Author = {Maheshri, Vikram and Winston, Clifford},
Title = {{Did the Great Recession keep bad drivers off the road?}},
Journal = {{JOURNAL OF RISK AND UNCERTAINTY}},
Year = {{2016}},
Volume = {{52}},
Number = {{3}},
Pages = {{255-280}},
Month = {{JUN}},
Abstract = {{Motorists' fatalities and the fatality rate (roadway deaths per
   vehicle-mile traveled (VMT)) tend to decrease during recessions. Using a
   novel data set of individual drivers, we establish that recessions have
   differential impacts on driving behavior by decreasing the VMT of
   observably risky drivers, such as those over age 60, and by increasing
   the VMT of observably safer drivers. The compositional shift toward
   safer drivers associated with a one percentage point increase in
   unemployment would save nearly 5000 lives per year nationwide. This
   finding suggests that policymakers could generate large benefits by
   targeting new driver-assistance technology at vulnerable groups.}},
