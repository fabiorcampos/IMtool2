Language = {{English}},
Affiliation = {{Minaeian, S (Reprint Author), Univ Arizona, Syst \& Ind Engn Dept, Tucson, AZ 85721 USA.
   Minaeian, Sara; Liu, Jian; Son, Young-Jun, Univ Arizona, Syst \& Ind Engn Dept, Tucson, AZ 85721 USA.}},
DOI = {{10.1109/TSMC.2015.2491878}},
