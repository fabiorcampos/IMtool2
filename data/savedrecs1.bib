@article{ ISI:000380603200107,
Author = {Overmeyer, Ludger and Podszus, Florian and Dohrmann, Lars},
Title = {{Multimodal speech and gesture control of AGVs, including EEG-based
   measurements of cognitive workload}},
Journal = {{CIRP ANNALS-MANUFACTURING TECHNOLOGY}},
Year = {{2016}},
Volume = {{65}},
Number = {{1}},
Pages = {{425-428}},
Note = {{66th General Assembly of the
   International-Academy-for-Production-Engineering (CIRP), Guimaraes,
   PORTUGAL, AUG 21-27, 2016}},
Organization = {{Int Acad Prod Engn}},
Abstract = {{Automated guided vehicles (AGVs) with autonomous behavior and
   decentralized human-machine interaction (HMI) are suitable for use in
   logistics. To facilitate natural interaction, HMI may involve both
   speech and gesture control.This paper presents a new cognitive approach
   based on electroencephalography (EEG) for multimodal HMI combining
   speech and gesture control for AGVs used in logistics. The results
   indicate that implicit EEG-based measures such as alertness and
   relaxation significantly affect speech control performance.
   Consequently, monitoring the user's cognitive workload during logistic
   operations may lead to a substantial improvement in work performance.
   (C) 2016 CIRP.}},
