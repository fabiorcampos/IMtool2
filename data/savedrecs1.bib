@article{ ISI:000368189000004,
Author = {Elbanhawi, Mohamed and Simic, Milan and Jazar, Reza N.},
Title = {{Continuous Path Smoothing for Car-Like Robots Using B-Spline Curves}},
Journal = {{JOURNAL OF INTELLIGENT \& ROBOTIC SYSTEMS}},
Year = {{2015}},
Volume = {{80}},
