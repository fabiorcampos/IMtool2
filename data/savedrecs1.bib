@article{ ISI:000381146300006,
Author = {Charabaruk, Nicholas and Nokleby, Scott},
Title = {{DESIGN AND DEVELOPMENT OF AN AUTONOMOUS OMNIDIRECTIONAL HAZARDOUS
   MATERIALS HANDLING ROBOT}},
Journal = {{TRANSACTIONS OF THE CANADIAN SOCIETY FOR MECHANICAL ENGINEERING}},
