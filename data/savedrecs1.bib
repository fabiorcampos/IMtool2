@article{ ISI:000375556300015,
Author = {Attari, Mina and Luo, Zhongzhen and Habibi, Saeid},
Title = {{An SVSF-Based Generalized Robust Strategy for Target Tracking in Clutter}},
Journal = {{IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS}},
