@article{ ISI:000358585200015,
Author = {Cichella, Venanzio and Kaminer, Isaac and Dobrokhodov, Vladimir and
   Xargay, Enric and Choe, Ronald and Hovakimyan, Naira and Aguiar, A.
   Pedro and Pascoal, Antonio M.},
Title = {{Cooperative Path Following of Multiple Multirotors Over Time-Varying
   Networks}},
