@article{ ISI:000362084600041,
Author = {Yoon, Sangyol and Yoon, Sung-Eui and Lee, Unghui and Shim, David
   Hyunchul},
Title = {{Recursive Path Planning Using Reduced States for Car-Like Vehicles on
   Grid Maps}},
