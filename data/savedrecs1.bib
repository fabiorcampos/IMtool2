@article{ ISI:000356363900003,
Author = {Gordon, T. J. and Lidberg, M.},
Title = {{Automated driving and autonomous functions on road vehicles}},
Journal = {{VEHICLE SYSTEM DYNAMICS}},
Year = {{2015}},
Volume = {{53}},
Number = {{7, SI}},
Pages = {{958-994}},
