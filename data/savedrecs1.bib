@article{ ISI:000368942000006,
Author = {Warren, Michael and Mejias, Luis and Kok, Jonathan and Yang, Xilin and
   Gonzalez, Felipe and Upcroft, Ben},
Title = {{An Automated Emergency Landing System for Fixed-Wing Aircraft: Planning
   and Control}},
Journal = {{JOURNAL OF FIELD ROBOTICS}},
