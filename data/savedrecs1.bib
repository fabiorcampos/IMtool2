@article{ ISI:000372270400022,
Author = {Liang, Xiao and Li, Ye and Peng, Zhouhua and Zhang, Jundong},
Title = {{Nonlinear dynamics modeling and performance prediction for underactuated
   AUV with fins}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2016}},
Volume = {{84}},
Number = {{1, SI}},
Pages = {{237-249}},
Month = {{APR}},
Abstract = {{The application of autonomous underwater vehicles (AUV) is widespread
   with the development of the activities in deep ocean. For low energy
   consumption, low resistance and excellent maneuverability, fins are
   usually used to modify AUV hydrodynamic forces. The underactuated AUV
   can do gyratory motion by vertical fins and do diving and rising motion
   by horizontal fins. However, the modeling and performance prediction for
   the underactuated AUV with fins have not been solved well. We build the
   dynamics and kinematics model in 6-DOF for the underactuated AUV with
   fins and analyze the forces and hydrodynamic coefficients in detail,
   especially the fin effect. Various hull hydrodynamic coefficients are
   obtained by CFD numerical computation. A series of simulation
   experiments are conducted to predict AUV hydrodynamic performance, and
   the feasibility and accuracy are verified by comparing the results on
   WL-II underactuated AUV in ocean experiments.}},
