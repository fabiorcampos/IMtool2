@article{ ISI:000379354200013,
Author = {Bharamagoudra, Manjula R. and Manvi, Sunil Kumar S.},
Title = {{Deployment Scheme for Enhancing Coverage and Connectivity in Underwater
   Acoustic Sensor Networks}},
Journal = {{WIRELESS PERSONAL COMMUNICATIONS}},
