@article{ ISI:000362084600033,
Author = {Liu, Yonggui and Xu, Bugong},
Title = {{Improved Protocols and Stability Analysis for Multivehicle Cooperative
   Autonomous Systems}},
Journal = {{IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS}},
Year = {{2015}},
Volume = {{16}},
