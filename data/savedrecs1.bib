@article{ ISI:000381456400019,
Author = {de Lima, Danilo Alves and Victorino, Alessandro Correa},
Title = {{A Hybrid Controller for Vision-Based Navigation of Autonomous Vehicles
   in Urban Environments}},
Journal = {{IEEE TRANSACTIONS ON INTELLIGENT TRANSPORTATION SYSTEMS}},
Year = {{2016}},
Volume = {{17}},
