@article{ ISI:000365236700015,
Author = {Flores, V. and Villa, J. F. and Porta, M. A. and Gutierrez, J.},
Title = {{Shopping Market Assistant Robot}},
Journal = {{IEEE LATIN AMERICA TRANSACTIONS}},
Year = {{2015}},
Volume = {{13}},
Number = {{8}},
Pages = {{2559-2566}},
Month = {{AUG}},
Abstract = {{The implementation of a service robot for shopping-market assistant is
   presented. The scaled prototype robot is a four-wheeled differential
   vehicle based on microcontrollers and ultrasonic sensors for following a
   person to a certain distance while groceries are selected. Each grocery
   is identified by a unique number through radio frequency tags. The
   grocery is placed in the basket of the robot by the user, this
   identifier is read and transmitted to a remote computer by means of
   ZigBee wireless communication protocol, querying to a database for
   information about the grocery, which is presented to the user on a color
   display with the bill. The results in convenience stores have been shown
   that the autonomous robot is a reliable tool for shopping assistant.}},
