@article{ ISI:000359253600031,
Author = {Ma, Liang and Xue, Jianru and Kawabata, Kuniaki and Zhu, Jihua and Ma,
   Chao and Zheng, Nanning},
Title = {{Efficient Sampling-Based Motion Planning for On-Road Autonomous Driving}},
