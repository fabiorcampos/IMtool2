@article{ ISI:000365378800014,
Author = {Van-Dung Hoang and Jo, Kang-Hyun},
Title = {{Automatic Calibration of Camera and LRF based on Morphological Pattern
   and Optimal Angular Back-projection Error}},
Journal = {{INTERNATIONAL JOURNAL OF CONTROL AUTOMATION AND SYSTEMS}},
