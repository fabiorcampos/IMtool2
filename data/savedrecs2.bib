@article{ ISI:000352679500003,
Author = {Jurdak, Raja and Elfes, Alberto and Kusy, Branislav and Tews, Ashley and
   Hu, Wen and Hernandez, Emili and Kottege, Navinda and Sikka, Pavan},
Title = {{Autonomous surveillance for biosecurity}},
Journal = {{TRENDS IN BIOTECHNOLOGY}},
Year = {{2015}},
Volume = {{33}},
Number = {{4}},
Pages = {{201-207}},
Month = {{APR}},
Abstract = {{The global movement of people and goods has increased the risk of
   biosecurity threats and their potential to incur large economic, social,
   and environmental costs. Conventional manual biosecurity surveillance
   methods are limited by their scalability in space and time. This article
   focuses on autonomous surveillance systems, comprising sensor networks,
   robots, and intelligent algorithms, and their applicability to
   biosecurity threats. We discuss the spatial and temporal attributes of
   autonomous surveillance technologies and map them to three broad
   categories of biosecurity threat: (i) vector-borne diseases; (ii) plant
   pests; and (iii) aquatic pests. Our discussion reveals a broad range of
   opportunities to serve biosecurity needs through autonomous
   surveillance.}},
