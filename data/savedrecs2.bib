@article{ ISI:000349419400041,
Author = {Song, Jun-Beom and Byun, Young-Seop and Kim, Jeong and Kang, Beom-Soo},
Title = {{Guidance and control of a scaled-down quad tilt prop PAV}},
Journal = {{JOURNAL OF MECHANICAL SCIENCE AND TECHNOLOGY}},
