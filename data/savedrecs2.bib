@article{ ISI:000346911500006,
Author = {Revesz, Peter Z.},
Title = {{Artificial Intelligence Basic Research Directions at the US Air Force}},
Journal = {{INTERNATIONAL JOURNAL ON ARTIFICIAL INTELLIGENCE TOOLS}},
