@article{ ISI:000335961700008,
Author = {Li, Qingquan and Chen, Long and Zhu, Quanwen and Li, Ming and Zhang, Qun
   and Ge, Shuzhi Sam},
Title = {{Intersection detection and recognition for autonomous urban driving
   using a virtual cylindrical scanner}},
Journal = {{IET INTELLIGENT TRANSPORT SYSTEMS}},
