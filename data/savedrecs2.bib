@article{ ISI:000340295000008,
Author = {Peng, Zhouhua and Wang, Dan and Zhang, Hongwei and Sun, Gang},
Title = {{Distributed Neural Network Control for Adaptive Synchronization of
   Uncertain Dynamical Multiagent Systems}},
